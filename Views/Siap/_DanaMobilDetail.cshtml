                                    <ul class="list-group custom-list-group mb-n3">
                                        <li class="list-group-item align-items-center d-flex justify-content-between">
                                            <div class="media">
                                                <img src="@(siapurl + strCarPhoto)" class="me-3 align-self-center rounded" height="80">
                                                <div>&nbsp;&nbsp;</div>
                                                <div class="media-body align-self-center">
                                                    <a href="@(siapurl + strCarPhoto)" target="_blank" class="font-12 text-primary">View</a>
                                                </div><!--end media body-->
                                            </div>
                                        </li>
                                    </ul>
                                </div>
