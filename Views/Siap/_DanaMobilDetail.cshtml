                                    <input class="form-control" id="tenor" value="@(Model.Tenor)" readonly disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="installment" class="col-sm-4 col-form-label">Angsuran (IDR)</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="installment" value="@(strEstimatedInstallment)" readonly disabled>
                                    <input class="form-control" id="installmentdb" value="@(Model.EstimatedInstallment)" hidden />
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="installment" class="col-sm-4 col-form-label">Pendanaan (IDR)</label>
                                <div class="col-sm-8">
                                    <input class="form-control" id="disbursement" value="@(strEstimatedDisbursement)" readonly disabled>
                                    <input class="form-control" id="disbursementdb" value="@(Model.EstimatedDisbursement)" hidden />
