                                                <img src="@(siapurl + strNpwpPhoto)" class="me-3 align-self-center rounded" height="80">
                                                <div>&nbsp;&nbsp;</div>
                                                <div class="media-body align-self-center">
                                                    <a href="@(siapurl + strNpwpPhoto)" target="_blank" class="font-12 text-primary">View</a>
                                                </div><!--end media body-->
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="npwp" class="col-sm-4 col-form-label">Reason Reject</label>
                                <div class="col-sm-8">
                                    <textarea class="form-control" id="reasonreject" rows="4"></textarea>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-4">
                                </div>
                                <div class="col-sm-4">
                                    <form id="frmrejectdata">
                                        <button type="button" class="btn btn-danger px-3 mt-2" v-on:click="rejectDanaMobil()">Reject</button>
                                    </form>
                                </div>
                                <div class="col-sm-4">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Activity (Tracking Status)</h4>
                    </div><!--end card-header-->
                    <div class="card-body">

                        <div class="slimscroll activity-scroll">
                            <div class="activity">
                                <div class="activity-info">
                                    <div class="icon-info-activity">
                                        <i class="las la-edit"></i>
                                    </div>
                                    <div class="activity-info-text">
                                        <div class="card report-card">
                                            <div class="card-body">
                                                <div class="row d-flex justify-content-center">
                                                    <div class="col">
                                                        <p class="text-muted mb-0 fw-semibold">
                                                            Status :                                                         
                                                        </p>
                                                        <h3 class="text-muted m-0">
                                                            Entry Data                                                            
                                                        </h3>                                                        
                                                    </div>                                                    
                                                </div>
                                            </div><!--end card-body--> 
                                        </div>                                        
                                    </div>
                                </div> 

                                @{
                                    if (sts == "push-to-mfin")
                                    {
                                        stsbtnitrack = "";
                                        lblsts = sts;
                                        iconactivity = " bg-soft-primary";
                                        textactivity = "text-dark ";
                                        cbtnitrack = "btn btn-sm btn-primary px-3 mt-2";
                                    }
                                    else
                                    {
                                        stsbtnitrack = "disabled";
                                        lblsts = "";
                                        iconactivity = "";
                                        textactivity = "text-muted";
                                        cbtnitrack = "btn btn-sm btn-outline-secondary px-3 mt-2";
                                    }
                                }
                                <div class="activity-info">
                                    <div class="icon-info-activity">
                                        <i class="las la-mouse-pointer @(iconactivity)"></i>
                                    </div>
                                    <div class="activity-info-text">
                                        <div class="card report-card">
                                            <div class="card-body">
                                                <div class="row d-flex justify-content-center">
                                                    <div class="col">
                                                        <p class="@(textactivity) mb-0 fw-semibold">
                                                            Status : @(lblsts)                                                                                                                        
                                                        </p>
                                                        <h3 class="@(textactivity) m-0">
                                                            Push iTrack                                                            
                                                        </h3>                                                        
                                                    </div>                                                    
                                                </div>
                                                <hr class="hr-dashed">
                                                <form id="frmpushitrack">                                                    
                                                    <input class="form-control" id="sts" value="@(sts)" hidden />
                                                    <button type="button" class="@(cbtnitrack)" v-on:click="submitItrack()" @(stsbtnitrack)>Submit iTrack</button>
                                                </form>
                                            </div><!--end card-body--> 
                                        </div>                                        
                                    </div>
                                </div>

                                @{
                                    if (sts == "itrack-submit")
                                    {
                                        stsbtnsurveyprogress = "";
                                        cbtnsurveyprogress = "btn btn-sm btn-warning px-3 mt-2";
                                    }
                                    else
                                    {
                                        stsbtnsurveyprogress = "disabled";
                                        cbtnsurveyprogress = "btn btn-sm btn-outline-secondary px-3 mt-2";
                                    }

                                    if (sts == "survey-onprogress")
                                    {
                                        stsbtnsurveydone = "";
                                        cbtnsurveydone = "btn btn-sm btn-success px-3 mt-2";
                                    }
                                    else
                                    {
                                        stsbtnsurveydone = "disabled";
                                        cbtnsurveydone = "btn btn-sm btn-outline-secondary px-3 mt-2";
                                    }

                                    if (sts == "itrack-submit" || sts == "survey-onprogress")
                                    {
                                        lblsts = sts;
                                        iconactivity = " bg-soft-primary";
                                        textactivity = "text-dark ";
                                        
                                    }
                                    else
                                    {
                                        lblsts = "";
                                        iconactivity = "";
                                        textactivity = "text-muted";
                                    }
                                }
                                <div class="activity-info">
                                    <div class="icon-info-activity">
                                        <i class="las la-car @(iconactivity)"></i>
                                    </div>
                                    <div class="activity-info-text">
                                        <div class="card report-card">
                                            <div class="card-body">
                                                <div class="row d-flex justify-content-center">
                                                    <div class="col">
                                                        <p class="@(textactivity) mb-0 fw-semibold">
                                                            Status : @(lblsts)                                                            
                                                        </p>
                                                        <h3 class="@(textactivity) m-0">
                                                            Survey Data
                                                        </h3>
                                                    </div>
                                                </div>
                                                <hr class="hr-dashed">
                                                <form id="frmsurveydata">
                                                    <input class="form-control" id="sts" value="@(sts)" hidden />
