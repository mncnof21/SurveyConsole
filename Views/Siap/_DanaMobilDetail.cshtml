                                                        <label for="horizontalInput1" class="col-sm-2 form-label align-self-center mb-lg-0">Survey Date</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" class="form-control" id="surveydate" value="@(Model.SurveyDate)" readonly />
                                                        </div>
                                                    </div>

                                                    <div class="mb-3 row">
                                                        <label for="horizontalInput2" class="col-sm-2 form-label align-self-center mb-lg-0">Surveyor</label>
                                                        <div class="col-sm-10">
                                                            <select class="form-control" id="surveyor" name="surveyor" @(stsbtnsurveyprogress)>
                                                                <option value="">-- select</option>
                                                                <option value="MNCF">MNCF</option>
                                                                <option value="POS">POS</option>
                                                            </select>
                                                        </div>
                                                    </div>                                                    
